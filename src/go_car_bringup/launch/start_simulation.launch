<?xml version='1.0' encoding='UTF-8'?>
<launch>

    <arg name="world_name" default="worlds/empty.world"/>
    <arg name="debug" default="false"/>

    <!-- <param name="/use_sim_time" value="true" /> -->
    <!-- <node name="gazebo_simulation" pkg="gazebo_ros" type="gazebo" args="$(find gazebo_ros)/worlds/empty.world" respawn="false" output="screen"/> -->
    <include file="$(find gazebo_ros)/launch/empty_world.launch">
        <arg name="world_name" value="$(arg world_name)"/>
        <arg name="debug" value="$(arg debug)"/>
        <arg name="gui" value="true"/>
        <arg name="paused" value="false"/>
        <arg name="use_sim_time" value="true"/>
        <arg name="headless" value="false"/>
    </include>

    <param name="robot_description" command="$(find xacro)/xacro '$(find go_car_description)/urdf/go_car.urdf.xacro' robot_name:='GoCar'"/>
    <node name="urdf_spawner" pkg="gazebo_ros" type="spawn_model" output="screen" respawn="false" args="-urdf -model GoCar -param robot_description -x -0.08 -y -0.09 -z 0.785"/>

    <!-- <include file="$(arg control_launch_file)">
        <arg name="robot_name" value="catvehicle"/>
        <arg name="config_file" value="catvehicle_control.yaml"/>
    </include> -->

    <!-- <include file="$(find can_sim_tc)/launch/basic_offline_cansimnodelet.launch"/> -->

    <!-- <node name="car_controls_to_cmdvel" pkg="catvehicle" type="car_controls_to_cmdvel.py" respawn="false" output="screen"/> -->

</launch>